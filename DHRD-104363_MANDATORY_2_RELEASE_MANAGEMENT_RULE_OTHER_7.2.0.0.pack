if (res = -1 or res = 0) {
 return;
}
//get Package Release code
release_code := obj.CODE;

//launch the task with the correct argument
task := getObjectByCode("SCHEDTASK", "INSTALL_PACKAGE_RELEASE");
args := array();
arrayappend(args, array("RELEASE_CODE", release_code));
msg := task.immediateRequest(args);

if( msg = '"Unable to find the scheduler process associated to the task."' and isinteractive())
{
  userinfo(msg +".\nPlease start a scheduler before launching a task");
}
else
{
  if (isinteractive() and msg != "")
    userinfo(msg);
}]]></IMPACT>
