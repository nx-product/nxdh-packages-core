<IMPACT ><![CDATA[//Reset the list of control linked to the execution rule
if (obj.first("CONTROLEXECRULE_CONTROL")){
   do {
      obj.remove("CONTROLEXECRULE_CONTROL");
   } while (obj.next("CONTROLEXECRULE_CONTROL"))
}
technicalClass := '';
if (obj.BUSINESS_CLASS != ""){
   businessClass := getObject("MxBusinessClass", obj.BUSINESS_CLASS);
   technicalClass := getObject("MxClassDef", businessClass.TECH_CLASS);
} else if (obj.TECHNICAL_CLASS != ""){
   technicalClass := getObject("MxClassDef", obj.TECHNICAL_CLASS);
}
if (technicalClass != '') {
   table := technicalClass.TABLE_NAME[REC_TYPE=0];
   condition := ',[[["'+table+'",0,"",?,0,0,]][]]';
   obj.CONDITION := condition;
}
//saveObjects(obj);]]></IMPACT>
